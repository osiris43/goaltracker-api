<h1>Listing timeframes</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @timeframes.each do |timeframe| %>
      <tr>
        <td><%= timeframe.description %></td>
        <td><%= link_to 'Show', timeframe %></td>
        <td><%= link_to 'Edit', edit_timeframe_path(timeframe) %></td>
        <td><%= link_to 'Destroy', timeframe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timeframe', new_timeframe_path %>
